{
  "name": "frontend-code-generator-mcp",
  "version": "1.0.0",
  "description": "MCP simple para generar código frontend común",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^26.1.0",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "mcp",
    "frontend",
    "react",
    "vue",
    "code-generator"
  ],
  "author": "Anderson Romero",
  "license": "MIT"
}
